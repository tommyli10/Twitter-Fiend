{
    "manifest_version" : 3,
		"name" : "Twitter_Fiend",
		"version" : "1.0",
		"description" : "For tweetaholics. Tweet faster. Anywhere",
		"icons" : {"227" : "Icon.png"},
		"action": {"default_popup": "popup.html", "default_icon": "Icon.png"},
        "background": {"service_worker": "background.js"},
		"permissions": ["storage", "activeTab", "scripting", "tabs"]
}